{
    "version": 2,
    "builds": [
        {
            "src": "./dist/server.js",
            "use": "@vercel/node", 
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./dist/server.js",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "headers": [
                {
                    "key": "access-control-allow-origin",
                    "value": "https://mo-news.vercel.app/"
                }
            ]
        }
    ]
}